{
  "name": "real-link",
  "version": "2.0.0",
  "description": "A Chrome extension that automatically restores external links on Chinese websites, bypassing redirect intercepts",
  "main": "dist/background.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "dev": "npm run build:dev && npm run watch"
  },
  "keywords": [
    "chrome-extension",
    "typescript",
    "link-modification",
    "external-links",
    "redirect-bypass",
    "zhihu",
    "juejin",
    "csdn",
    "jianshu"
  ],
  "author": "berlin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/realLink.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/realLink/issues"
  },
  "homepage": "https://github.com/yourusername/realLink#readme",
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.0.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0"
  }
}
